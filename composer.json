{
  "name": "prezent/push-bundle",
  "type": "symfony-bundle",
  "description": "Symfony bundle the sent push notification through various providers",
  "keywords": ["push", "mobile", "bundle", "pushwoosh", "onesignal"],
  "homepage": "http://www.prezent.nl",
  "require": {
    "php": ">=5.5.9"
  },
  "require-dev": {
    "phpunit/phpunit": "@stable",
    "symfony/console": "~2.3|~3.0",
    "symfony/framework-bundle": "~2.3|~3.0"
  },
  "suggest": {
    "doctrine/orm": "To use a database to store the push receivers in",
    "gomoob/php-pushwoosh": "To sent notification through Pushwoosh",
    "norkunas/onesignal-php-api": "To sent notification through OneSignal"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Robert-Jan Bijl",
      "email": "robert-jan@prezent.nl"
    },
    {
      "name": "Prezent Internet B.V.",
      "email": "github@prezent.nl"
    }
  ],
  "autoload": {
    "psr-4": { "Prezent\\PushBundle\\": "" }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.1.x-dev"
    }
  }
}